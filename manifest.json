{
  "manifest_version": 3,
  "name": "Luht Speed Tracker v2",
  "version": "5.0.0",
  "description": "Статистика скорости разметки, красивый розовый UI и турбо-ускорение сайта",
  "icons": {
    "128": "icon128.png"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://luht.tp2.intropy.tech/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://luht.tp2.intropy.tech/v2/*"
      ],
      "css": [
        "styles.css"
      ],
      "js": [
        "injector.js",
        "core.js",
        "freezer.js",
        "watchdog.js"
      ],
      "run_at": "document_start",
      "all_frames": false
    }
  ]
}
